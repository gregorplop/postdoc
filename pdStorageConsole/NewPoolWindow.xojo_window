#tag Events PushButton1
	#tag Event
		Sub Action()
		  newPoolObj = new pdstorage_pool
		  
		  newPoolObj.name = nameField.Text
		  newPoolObj.friendlyName = friendlyNameField.text
		  newPoolObj.comments = commentsField.Text
		  newPoolObj.mediumThreshold = mediaLimitField.Text.Val
		  newPoolObj.rootFolder = GetFolderItem(rootFolderField.Text)
		  newPoolObj.autoClose = autoCloseCheck.Value
		  newPoolObj.autoExpand = autoExpandCheck.Value
		  
		  self.close
		  
		End Sub
	#tag EndEvent
#tag EndEvents
