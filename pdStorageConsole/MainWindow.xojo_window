		Sub DoubleClick()
		  dim row as integer = me.ListIndex
		  dim ff as FolderItem
		  
		  if row < 0 then exit sub
		  
		  if vfs = nil then  // no loaded vfs
		    
		    select case me.cell(row,0)
		      
		    case "VFS file path"
		      tempString = me.cell(row,1)
		      me.EditCell(row,1)
		      
		    case "Password"
		      me.EditCell(row,1)
		      
		    case "Name"
		      me.EditCell(row,1)
		      
		    case "Friendly name"
		      me.EditCell(row,1)
		      
		    case "Description"
		      me.EditCell(row,1)
		      
		    end select
		    
		  else  // there is a vfs loaded
		    
		    
		    
		    
		  end if
		End Sub
