		Function ConstructContextualMenu(base as MenuItem, x as Integer, y as Integer) As Boolean
		  if IsNull(vfs) = false then return true
		  dim row as integer = me.RowFromXY(x,y)
		  if row < 0 then return true
		  
		  
		  if me.cell(row,0) = "VFS file path" then
		    base.Append(New MenuItem("Select VFS file" , "SELECTVFSFILE"))
		  end if
		  
		  return true
		  
		End Function
	#tag EndEvent
	#tag Event
		Function ContextualMenuAction(hitItem as MenuItem) As Boolean
		  select case hitItem.Tag 
		    
		  case "SELECTVFSFILE"
		    
		    dim ff as FolderItem = GetOpenFolderItem("")
		    
		    if ff = nil then 
		      return true
		    else
		      me.CellTag(findRowHavingValue(me,0,"VFS file path") , 1) = ff
		      me.cell(findRowHavingValue(me,0,"VFS file path") , 1) = ff.NativePath
		      OpenVFS_btn.Enabled = true
		      
		    end if
		    
		    
		  end select
		  
		  return true
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub CellAction(row As Integer, column As Integer)
		  select case me.cell(row,0)
		    
		  case "VFS file path"
		    if me.cell(row,column).trim <> "" then
		      dim ff as FolderItem = GetFolderItem(me.cell(row,column))
		      if ff = nil then 
		        MsgBox "Invalid path"
		        me.cell(row,column) = tempString
		      else
		        me.CellTag(row,column) = ff
		        if ff.Exists = true then OpenVFS_btn.Enabled = true else CreateVFS_btn.Enabled = true
		      end if
		    end if
		    
		  end select
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean
		  If row Mod 2 = 0 Then
		    g.ForeColor = &cf3f6fA
		    g.FillRect(0, 0, g.Width, g.Height)
		  End If
		End Function
