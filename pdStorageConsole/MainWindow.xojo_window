#tag Events OpenVFS_btn
	#tag Event
		Sub Action()
		  if VFSlist.CellTag(findRowHavingValue(VFSlist,0,"VFS file path") , 1).IsNull = true then
		    MsgBox "Cannon open VFS: File is null"
		    exit sub
		  end if
		  
		  if VFSlist.CellTag(findRowHavingValue(VFSlist,0,"VFS file path") , 1) isa FolderItem = false then
		    MsgBox "Cannon open VFS: File is not a folder item"
		    exit sub
		  end if
		  
		  dim ff as FolderItem = VFSlist.CellTag(findRowHavingValue(VFSlist,0,"VFS file path") , 1)
		  dim openVFS as new pdstorage_vfs
		  
		  openVFS.DBfile = ff
		  openVFS.password = VFSlist.Cell(findRowHavingValue(VFSlist,0,"Password") , 1)
		  
		  vfs = new pdstorage_session_maintenance(openVFS)
		  
		  
		  if vfs.getInitOutcome.ok = true then
		    Footer.Text = ff.Name + " load OK"
		    
		    openVFS = vfs.public_getVFSdetails
		    vfsLabel.Text = openVFS.name
		    
		    VFSlist.cell(findRowHavingValue(VFSlist,0,"Name") , 1) = openVFS.name
		    VFSlist.cell(findRowHavingValue(VFSlist,0,"Friendly name") , 1) = openVFS.friendlyName
		    VFSlist.cell(findRowHavingValue(VFSlist,0,"pd Version") , 1) = openVFS.pdVersion
		    VFSlist.cell(findRowHavingValue(VFSlist,0,"Init stamp") , 1) = openVFS.initStamp.SQLDateTime
		    VFSlist.cell(findRowHavingValue(VFSlist,0,"Description") , 1) = openVFS.description
		    
		    OpenVFS_btn.Enabled = false
		    CloseVFS_btn.Enabled = true
		    CreateVFS_btn.Enabled = false
		    createPoolBtn.Enabled = true
		    seeMediaBtn.Enabled = true
		    NextMedium_Btn.Enabled = true
		    showAllMedia_btn.Enabled = true
		    
		    poolsCombo.DeleteAllRows
		    poolsCombo.AddRows vfs.getPoolNames
		    
		  else
		    
		    Footer.Text = ff.Name + " failed to load"
		    vfsLabel.Text = "no VFS"
		    
		    MsgBox vfs.getInitOutcome.fatalErrorMsg
		    
		    OpenVFS_btn.Enabled = true
		    CloseVFS_btn.Enabled = false
		    CreateVFS_btn.Enabled = False
		    createPoolBtn.Enabled = false
		    
		  end if
		  
		  AddHandler vfs.poolPasswordRequest , AddressOf passwordHandler
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CloseVFS_btn
	#tag Event
		Sub Action()
		  if IsNull(vfs) = false then vfs.Close
		  vfs = nil
		  Footer.Text = "VFS closed"
		  vfsLabel.Text = "No VFS"
		  
		  me.Enabled = false
		  CreateVFS_btn.Enabled = false
		  
		  poolsList.DeleteAllRows
		  createPoolBtn.Enabled = false
		  showAllMedia_btn.Enabled = false
		  NextMedium_Btn.Enabled = false
		  
		  
		  buildVFSlist
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CreateVFS_btn
	#tag Event
		Sub Action()
		  dim newVFS as new pdstorage_vfs
		  newVFS.DBfile = VFSlist.CellTag(findRowHavingValue(VFSlist,0,"VFS file path"),1)
		  newVFS.description = VFSlist.Cell(findRowHavingValue(VFSlist,0,"Description"),1)
		  newVFS.name = VFSlist.Cell(findRowHavingValue(VFSlist,0,"Name"),1)
		  newVFS.friendlyName = VFSlist.Cell(findRowHavingValue(VFSlist,0,"Friendly name"),1)
		  newVFS.password = VFSlist.Cell(findRowHavingValue(VFSlist,0,"Password"),1)
		  
		  dim Outcome as pdOutcome = pdstorage_maintenance.initVFS(newVFS)
		  
		  if Outcome.ok = true then
		    Footer.Text = "VFS created OK"
		    me.Enabled = false
		    OpenVFS_btn.Enabled = true
		    CloseVFS_btn.Enabled = false
		    
		  else
		    
		    MsgBox Outcome.fatalErrorMsg
		    
		  end if
		  
		  
		End Sub
