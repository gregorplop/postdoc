	#tag Method, Flags = &h0
		Sub buildMediaList(pool as string)
		  dim media(-1) as pdstorage_medium
		  dim pools(-1) as string
		  dim selection as integer = mediaList.ListIndex
		  dim row(6) as string
		  
		  if pool = "" then 
		    pools = vfs.getPoolNames
		  else
		    pools.Append pool
		  end if
		  
		  mediaList.DeleteAllRows
		  
		  for i as integer = 0 to pools.Ubound
		    media = vfs.public_getMediaDetails(pools(i))
		    
		    for j as integer = 0 to media.Ubound
		      row(0) = media(j).pool
		      row(1) = str(media(j).idx)
		      row(2) = media(j).folder.NativePath
		      row(3) = media(j).initStamp.SQLDateTime
		      row(4) = str(media(j).threshold)
		      row(5) = str(media(j).file.Length / MByte)
		      row(6) = str(media(j).open)
		      
		      mediaList.AddRow row
		    next j
		  next i
		  
		  if selection >= 0 then mediaList.ListIndex = selection
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub buildPoolsList()
		  dim selection as integer = poolsList.ListIndex
		  
		  poolsList.DeleteAllRows
		  
		  dim pool as pdstorage_pool
		  dim pools(-1) as string = vfs.getPoolNames
		  dim row(7) as string
		  for i as integer = 0 to pools.Ubound
		    pool = vfs.public_getPoolDetails(pools(i))
		    row(0) = pool.name
		    row(1) = pool.friendlyName
		    row(2) = pool.comments
		    row(3) = pool.rootFolder.NativePath
		    row(4) = str(pool.mediumThreshold)
		    row(5) = pool.initStamp
		    row(6) = str(pool.autoExpand)
		    row(7) = str(pool.autoClose)
		    poolsList.AddRow row
		  next i
		  
		  if selection >= 0 then poolsList.ListIndex = selection
		End Sub
	#tag EndMethod

