		Sub Action(index as Integer)
		  for i as integer = 0 to 3
		    if i <> index then FunctionBtn(i).Value = false
		  next i
		  
		  
		  select case index
		    
		  case 1
		    if IsNull(vfs) = false then
		      buildPoolsList
		    end if
		    MediaInfoRefresh.Mode = timer.ModeOff
		    
		  case 2
		    if IsNull(vfs) = false then
		      MediaInfoRefresh.Mode = timer.ModeMultiple
		    end if
		    
		  else
		    MediaInfoRefresh.Mode = timer.ModeOff
		    
		    
		  end select
		  
		  
		  MainPanel.Value = index
		  
		  
		End Sub
